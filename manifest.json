{
  "manifest_version": 2,
  "name": "Casinomucho Launcher",
  "description": "Quick launch for casinomucho.com Media",
  "version": "1.0.0",
  "icons": {
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ]
}